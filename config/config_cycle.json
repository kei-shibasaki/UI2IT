{
    "trainA_path": "datasets/horse2zebra/trainA",
    "trainB_path": "datasets/horse2zebra/trainB",
    "testA_path": "datasets/horse2zebra/testA",
    "testB_path": "datasets/horse2zebra/testB",
    "data_extention": "jpg",
    "save_extention": "jpg",
    "cache_images": true,
    "enable_line_nortify": true,

    "name": "TEST_cycle",
    "input_resolution": [256, 256],
    "network_module_G": "models.resnet",
    "model_type_G": "ResnetGenerator",
    "netG": {
        "input_nc": 3, 
        "output_nc": 3, 
        "ngf": 64, 
        "n_blocks": 9
    },
    "network_module_G_BA": "models.resnet",
    "model_type_G_BA": "ResnetGenerator",
    "netG_BA": {
        "input_nc": 3, 
        "output_nc": 3, 
        "ngf": 64, 
        "n_blocks": 9
    },
    "network_module_D": "models.discriminator",
    "model_type_D": "NLayerDiscriminator",
    "netD": {
        "input_nc": 3,
        "ndf": 64,
        "n_layers": 3
    },
    "network_module_D_BA": "models.discriminator",
    "model_type_D_BA": "NLayerDiscriminator",
    "netD_BA": {
        "input_nc": 3,
        "ndf": 64,
        "n_layers": 3
    },

    "batch_size": 1,
    "steps": 300000,
    "use_grad_clip": false,
    "grad_clip_val": 0.1,
    "coef_adv": 1,
    "coef_cycle": 10,
    
    "lr_w": 5e-5, 
    "lr_max": 1e-4, 
    "lr_min": 1e-7, 
    "step_w": 50000, 
    "step_max": 300000,
    "learning_rate": 1e-4,
    "betas": [0.5, 0.99], 
    "save_compare": true,

    "print_freq": 100,
