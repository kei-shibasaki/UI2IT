{
    "trainA_path": "datasets/horse2zebra/trainA",
    "trainB_path": "datasets/horse2zebra/trainB",
    "testA_path": "datasets/horse2zebra/testA",
    "testB_path": "datasets/horse2zebra/testB",
    "data_extention": "jpg",
    "save_extention": "jpg",
    "cache_images": true,
    "enable_line_nortify": true,

    "name": "TEST_LPTN",
    "input_resolution": [256, 256],
    "network_module_G": "models.lptn",
    "model_type_G": "LPTN",
    "netG": {
        "nrb_low": 5,
        "nrb_high": 3,
        "num_high": 3
    },

    "network_module_D": "models.discriminator",
    "model_type_D": "NLayerDiscriminator",
    "netD": {
        "input_nc": 3,
        "ndf": 64,
        "n_layers": 3
    },

    "batch_size": 32,
    "steps": 300000,
    "use_grad_clip": false,
    "grad_clip_val": 0.1,
    "coef_adv": 1,
    "coef_gp": 10,
    "coef_recons": 10,
    
    "lr_w": 5e-5, 
    "lr_max": 1e-4, 
    "lr_min": 1e-7, 
    "step_w": 50000, 
    "step_max": 300000,
    "learning_rate_G": 1e-4,
    "learning_rate_D": 1e-4,
    "betas": [0.9, 0.99], 
    "save_compare": true,

    "print_freq": 100,
    "eval_freq": 2000,
    "save_freq": 50000,
    "pretrained_path": null,
    "resume_step": 0,
    "create_new_log": true
}