{
    "trainA_path": "datasets/apple2orange/trainA",
    "trainB_path": "datasets/apple2orange/trainB",
    "testA_path": "datasets/apple2orange/testA",
    "testB_path": "datasets/apple2orange/testB",
    "trainA_mask_path": "datasets/apple2orange/trainA_map",
    "trainB_mask_path": "datasets/apple2orange/trainB_map",
    "testA_mask_path": "datasets/apple2orange/testA_map",
    "testB_mask_path": "datasets/apple2orange/testB_map",
    "data_extention": "jpg",
    "mask_extention": "png",
    "save_extention": "jpg",
    "cache_images": true,
    "enable_line_nortify": true,

    "name": "ours_apple2orange_saladv_lr_sep_recons_idt",
    "input_resolution": [256, 256],
    "network_module_G": "models.network",
    "model_type_G": "GeneratorSeparate",
    "netG": {
        "in_channels": 4,
        "out_channels": 6,
        "width": 32,
        "enc_blk_nums": [1,1,1], 
        "middle_blk_num": 3,
        "dec_blk_nums": [1,1,1]
    },

    "network_module_D": "models.discriminator",
    "model_type_D": "NLayerDiscriminator",
    "netD": {
        "input_nc": 3,
        "ndf": 64,
        "n_layers": 3
    },

    "network_module_D_mask": "models.discriminator",
    "model_type_D_mask": "NLayerDiscriminator",
    "netD_mask": {
        "input_nc": 1,
        "ndf": 32,
        "n_layers": 3
    },

    "netP": {
        "grid_size": 2,
        "crop_size": 256, 
        "r1": 0.9, 
        "r2": 0.9,
        "pert_threshold": 2.0
    },

    "batch_size": 1,
    "steps": 210000,
    "use_grad_clip": false,
    "grad_clip_val": 0.1,
    "coef_adv": 1,
    "coef_mspc": 1,
    "coef_constraint": 0.5,
    "coef_idt": 1,
    "coef_recons": 1,
    "coef_mask_recons": 1,
    "coef_adv_mask": 1,
    
    "lr_w": 5e-5, 
    "lr_max": 2e-4, 
    "lr_min": 0, 
    "step_w": 21000,
    "step_down": 105000, 
    "step_max": 210000,
    "learning_rate": 2e-4,
    "betas": [0.5, 0.999], 
    "save_compare": true,

    "print_freq": 200,
    "eval_freq": 5000,
    "save_freq": 10000,
    "nortify_freq": 100000,
    "pretrained_path": null,
    "resume_step": 0,
    "create_new_log": true
}