{
    "trainA_path": "datasets/selfie2anime/trainA",
    "trainB_path": "datasets/selfie2anime/trainB",
    "testA_path": "datasets/selfie2anime/testA",
    "testB_path": "datasets/selfie2anime/testB",
    "trainA_mask_path": "datasets/selfie2anime/trainA_map",
    "testA_mask_path": "datasets/selfie2anime/testA_map",
    "trainB_mask_path": "datasets/selfie2anime/trainB_map",
    "testB_mask_path": "datasets/selfie2anime/testB_map",
    "data_extention": "jpg",
    "mask_extention": "png",
    "save_extention": "jpg",
    "cache_images": false,
    "enable_line_nortify": true,

    "name": "ours_selfie2anime_foreonly_adv_mask_recons",
    "input_resolution": [256, 256],
    "network_module_G": "models.network",
    "model_type_G": "GeneratorSeparate",
    "netG": {
        "in_channels": 4,
        "width": 32,
        "enc_blk_nums": [1,1,1], 
        "middle_blk_num": 3,
        "dec_blk_nums": [1,1,1]
    },

    "network_module_D": "models.discriminator",
    "model_type_D": "NLayerDiscriminator",
    "netD": {
        "input_nc": 3,
        "ndf": 64,
        "n_layers": 3
    },

    "network_module_D_mask": "models.discriminator",
    "model_type_D_mask": "NLayerDiscriminator",
    "netD_mask": {
        "input_nc": 1,
        "ndf": 32,
        "n_layers": 3
    },

    "netP": {
        "grid_size": 2,
        "crop_size": 256, 
        "r1": 0.9, 
        "r2": 0.9,
        "pert_threshold": 2.0
    },

    "batch_size": 1,
    "steps": 680000,
    "use_grad_clip": true,
    "grad_clip_val": 0.1,
    "pool_size": 50,
    "coef_adv": 1,
    "coef_mspc": 1,
    "coef_constraint": 0.5,
    "coef_idt": 1,
    "coef_recons": 1,
    "coef_mask_recons": 1,
    "coef_adv_mask": 1,
    
    "lr_w": 5e-5, 
    "lr_max": 2e-4, 
    "lr_min": 0, 
    "step_w": 68000,
    "step_down": 340000, 
    "step_max": 680000,
    "learning_rate": 2e-4,
    "betas": [0.5, 0.999], 
    "save_compare": true,

    "print_freq": 100,
    "eval_freq": 10000,
    "save_freq": 50000,
    "nortify_freq": 100000,
    "pretrained_path": null,
    "resume_step": 0,
    "create_new_log": true
}